<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - All Right Reserved</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header>
        <div class="logo1">
            COMMONWEAL
        </div>
      
        <nav>
            <ul id="menu">
                <li>@Html.ActionLink("Home","Index","Home")</li>
                <li>@Html.ActionLink("Login","login","Login")</li>
                <li>@Html.ActionLink("NGO Register","CreateNGO","ngoRegistration")</li>
                <li>@Html.ActionLink("USER Register","CreateUser","userRegistration")</li>

            </ul>
        </nav>
    </header>
    <div id="body" class="container">
        @RenderSection("featured", required: false)
        <section class="content-wrapper main-content clear-fix" style="width:100%;">
            @RenderBody()
        </section>
    </div>
    <footer class=" navbar-fixed-bottom">

        <div class="content-wrapper">
            <div class="float-left">
                <p>&copy; @DateTime.Now.Year - Commonweal</p>
            </div>
        </div>

    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
