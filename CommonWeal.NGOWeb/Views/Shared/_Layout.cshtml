<!DOCTYPE html>
<html lang="en">
<head>
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css">*@
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    @*<link href="~/Content/font-awesome.min.css" rel="stylesheet" />*@
    <meta charset="utf-8" />
    <title>@ViewBag.Title - All Right Reserved</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/sitecss")
    @Scripts.Render("~/bundles/jquery")
   <link href="http://cdn.rawgit.com/davidstutz/bootstrap-multiselect/master/dist/css/bootstrap-multiselect.css" rel="stylesheet" type="text/css" />
            <script src="http://cdn.rawgit.com/davidstutz/bootstrap-multiselect/master/dist/js/bootstrap-multiselect.js" type="text/javascript"></script>
</head>
    
<body id="bodycolor">
  <div class="container">
    <div class="row">
       <header class="navbar-fixed-top" role="navigation">

        <div class="row content-wrapper">
            <div class="col-md-6 col-sm-4 col-xs-4">
                <a href="@Url.Action("Index", "Home")" class="navbar-brand">
                    <img src="~/Images/lg.png" id="logoimage" class="col-md-offset-2" />
                    <br />
                    <span id="logofont">Commonweal</span>
                </a>
            </div>
         @* @if (User.Identity.IsAuthenticated)
           {
              @Html.Partial("_filter")
           }
         *@
            <div class="col-md-6  col-sm-8  col-md-push-4  col-xs-8">
                @Html.Partial("_LoginPartial")
            </div>

        </div>
   
   
    </header>
  </div>


    <div class="row" id="body">
        @RenderSection("featured", required: false)
        <section class="content-wrapper">
            @RenderBody()
        </section>
    </div>
<div class="row">
    <footer class=" navbar-fixed-bottom">
        <div class="content-wrapper row">
            <div class="page-footer col-md-4 col-md-offset-5">
                <p  id="footerText">&copy; @DateTime.Now.Year - Commonweal</p>
            </div>
        </div>
    </footer>
</div>



    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")

        @RenderSection("scripts", required: false)
</div>
</body>
</html>
