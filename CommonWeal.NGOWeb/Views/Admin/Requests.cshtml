@model IEnumerable<CommonWeal.Data.NGOUser>

<div class="row">
    <div class="col-md-12 ">
        <div class="requests">
            <table class="tableAdmin col-md-12">
                <thead class="tableheader col-md-12">
                    <tr>
                        <th class="col-md-3">NGO NAME</th>
                        <th class="col-md-3">NGO E-Mail</th>
                        <th class="col-md-4">NGO URL</th>
                        <th class="col-md-2">ACCEPT</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {                       
                    <tr>
                        <td class="col-md-3 adminTableData">@Html.DisplayFor(modelItem => item.NGOName)</td>
                        <td class="col-md-3 adminTableData"><a>@Html.DisplayFor(modelItem => item.NGOEmailID) </a></td>
                        <td class="col-md-4 adminTableData"><a>@Html.DisplayFor(modelItem => item.WebsiteUrl) </a>
                        <td class="col-md-2 adminTableData"><a href="/admin/Accept/@item.LoginID" class="fa fa-check fa-2x" aria-hidden="true" style="color:green"  ></a>
                    </tr>   
                }
                <i><a href="/Admin/" style="font-size: 16px;">Back to Dashboard</a></i>
            </table>
        </div>
    </div>
</div>
