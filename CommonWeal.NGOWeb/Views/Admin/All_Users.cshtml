@model IEnumerable<CommonWeal.Data.RegisteredUser>
 
 <div class="row">
        <div class="col-md-12 ">
            <div class="requests">
            <table class="tableAdmin col-md-12">
                          <thead class="tableheader">
                              <tr>
                              <th>User NAME</th>
                              <th >Email Id</th>
                              <th >BLOCK</th>
                              </tr>

                          </thead>
                      @foreach (var item in Model)
                      { 
                      
                         <tr>

                              <td class="tableData"> @Html.DisplayFor(modelItem => item.FirstName)</td>
                             <td class="tableData"><a> @Html.DisplayFor(modelItem => item.UserEmail) </a></td>
                             
                              <td class="tableData"><a href="/admin/BlockUsers/@item.LoginID" class="fa fa-ban fa-2x" aria-hidden="true" style="color:red; margin-left:24px;" onmouseover:></a></td>
                        
                         </tr>
               

                      }
                   <i><a href="/Admin/" style="font-size:16px;">Back to Dashboard</a></i>
                 
               </table>
             
              
            </div>
        </div>
   
     
    </div>
 