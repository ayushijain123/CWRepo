<div class="row">
    <div class="top-panel col-md-12">
        <div class="col-md-4">
            <a href="/Admin/Requests">
                <div class="media bg-primary request admin-button">
                    <div class="media-left side-request pull-left">
                        <i class="icon fa fa-user-plus fa-spin-hover" aria-hidden="true"></i>
                    </div>
                    <div class="media-body">
                        <h3>Total Request</h3>
                        <h4>Today 
                        <span class="pull-right">@ViewBag.COR</span>
                        </h4>
                    </div>
                </div>
            </a>
        </div>

        <div class="col-md-4">
            <a href="/Admin/Active_Users">
                <div class="media bg-primary active-ngo admin-button">
                    <div class="media-left side-active-ngo pull-left">
                        <i class="icon fa fa-rotate-right fa-spin" aria-hidden="true"></i>
                    </div>
                    <div class="media-body">
                        <h3>Active NGO</h3>
                        <h4>Today
                        <span class="pull-right">@ViewBag.COAN</span>
                        </h4>
                    </div>
                </div>
            </a>
        </div>

        <div class="col-md-4">
            <a href="/Admin/Blocked_Users">
                <div class="media bg-primary blocked-ngo admin-button">
                    <div class="media-left side-blocked-ngo pull-left">
                        <i class="icon fa fa-ban fa-spin-hover" aria-hidden="true"></i>
                    </div>
                    <div class="media-body">
                        <h3>Blocked NGO</h3>
                        <h4>Today
                        <span class="pull-right">@ViewBag.COBN</span>
                        </h4>
                    </div>
                </div>
            </a>
        </div>
    </div>
</div>

<div class="row">
    <div class="bottom-panel col-md-12">
        <div class="col-md-4">
            <a href="/Admin/All_Users">
                <div class="media bg-primary all-users admin-button">
                    <div class="media-left side-all-users pull-left">
                        <i class="icon fa fa-users fa-spin-hover" aria-hidden="true"></i>
                    </div>
                    <div class="media-body">
                        <h3>All Users</h3>
                        <h4>Today
                        <span class="pull-right">@ViewBag.COAL</span>
                        </h4>
                    </div>
                </div>
            </a>
        </div>

        <div class="col-md-4">
            <a href="/Admin/Blocked_NormalUsers">
                <div class="media bg-primary blocked-users admin-button">
                    <div class="media-left side-blocked-users pull-left">
                        <i class="icon fa fa-ban fa-spin-hover" aria-hidden="true"></i>
                    </div>
                    <div class="media-body">
                        <h3>Blocked Users</h3>
                        <h4>Today
                        <span class="pull-right">@ViewBag.COBU</span>
                        </h4>
                    </div>
                </div>
            </a>
        </div>

        <div class="col-md-4">
            <div class="media bg-primary warned-users">
                <div class="media-left side-warned-users pull-left">
                    <i class="icon fa fa-exclamation-triangle fa-spin-hover" aria-hidden="true"></i>
                </div>
                <div class="media-body">
                    <h3>Warned Users</h3>
                    <h4>Today
                        <span class="pull-right">15</span>
                    </h4>
                </div>
            </div>
        </div>
    </div>
</div>

