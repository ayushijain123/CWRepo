<body>
    <div class="panel panel-default">
        <div class="panel-heading" style="">
            <center><h3>NGO Registration Form</h3></center>
        </div>
        <div class="panel-body">
            <div id="ngo">
                <form name="NGORegistration" class="form-horizontal" action="/ngoRegistration/" method="post" enctype="multipart/form-data">
                    <br />
                    <br />

                    <div class="form-group">
                        <label class="control-label col-xs-4" for="fn">NGO Name:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_userName)<br />
                            @*<input type="text" class="form-control" name="ngo_userName" id="ngo name" placeholder="NGO name">*@
                            @Html.ValidationMessageFor(a => a.ngo_userName)

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="ln">Unique Id:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_UniqueuId)<br />
                            @* <input type="text" class="form-control" name="ngo_UniqueuId" id="Unique Id" placeholder="Unique Id">*@
                            @Html.ValidationMessageFor(a => a.ngo_UniqueuId)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="email">Email:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_Email)<br />
                            @*<input type="email" name="ngo_Email" class="form-control" id="email1" placeholder="Enter email">*@
                            @Html.ValidationMessageFor(a => a.ngo_Email)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="TelephoneNumber">Telephone No:</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_Telephone)<br />
                            @* <input id="TelephoneNumber" type="text" name="ngo_Telephone" class="form-control" placeholder="Telephone Number">*@
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="MobileNumber">Mobile No:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_mobileNumber)<br />
                            @*<input type="text" name="ngo_mobileNumber" class="form-control" id="MobileNumber" placeholder="Mobile Number">*@
                            @Html.ValidationMessageFor(a => a.ngo_mobileNumber)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="Address">Address:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_Address)<br />
                            @*<input type="text" class="form-control" name="ngo_Address" id="Address" placeholder="Address">*@
                            @Html.ValidationMessageFor(a => a.ngo_Address)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="CheifFunctionary/ChairmanName">Chairman Name:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_ChairmanPersonName)<br />
                            @* <input type="text" class="form-control" name="ngo_ChairmanPersonName" id="CheifFunctionaryChairmanName" placeholder="CheifFunctionary/Chairman Name" />*@
                            @Html.ValidationMessageFor(a => a.ngo_ChairmanPersonName)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="PhotoIdOfChairman">Photo Id Of Chairman:*</label>
                        <div class="col-xs-6">
                            @*  @Html.TextBoxFor(a => a.ngo_filengo_PhotoId)<br/>*@
                            @Html.TextBox("filengo_PhotoId", "", new { type = "file" })
                            @*   <input type="file" class="btn1 btn-default btn-file" name="filengo_PhotoId"  />*@

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="ParentOrganization">Parent Organization:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_ParentOrganisation)<br />
                            @*<input type="text" class="form-control" name="ngo_ParentOrganisation" id="ParentOrganization" placeholder="Parent Organization">*@
                            @Html.ValidationMessageFor(a => a.ngo_ParentOrganisation)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="RegisterWith">Register With:*</label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_RegisteredWith)<br />
                            @* <input type="text" class="form-control" name="ngo_RegisteredWith" id="RegisterWith" placeholder="Register With">*@
                            @Html.ValidationMessageFor(a => a.ngo_RegisteredWith)
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="RegisterationNumber">Registeration No:* </label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_RegistrationNumber)<br />
                            @* <input type="text" class="form-control" name="ngo_RegistrationNumber" id="RegisterationNumber" placeholder="Registeration Number" />*@
                            @Html.ValidationMessageFor(a => a.ngo_RegistrationNumber)
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-xs-4" for="StateOfRegistration">State Of Registration:*</label>
                        <div class="col-xs-6">
                            @Html.DropDownListFor(m => m.ngo_State, (SelectList)ViewBag.Country, new { @class = "form-control" })<br />
                            @Html.ValidationMessageFor(a => a.ngo_State)
                            @* <select id="state" name="ngo_State" class="form-control">

                                <option value=''>Select</option>
                            </select>*@
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-xs-4" for="CityOfRegistration">City Of Registration:*</label>
                        <div class="col-xs-6">
                            @Html.DropDownListFor(m => m.ngo_City, (SelectList)ViewBag.Country, new { @class = "form-control" })<br />
                            @Html.ValidationMessageFor(a => a.ngo_City)
                            @* <select id="city" name="ngo_City" class="form-control">
                                <option value=''>Select</option>
                            </select>*@
                        </div>
                    </div>


                    @*                    <div class="form-group">
                        <label class="control-label col-xs-4" for="DateOfRegistration">Date Of Registration:* </label>
                        <div class="col-xs-6">
                        @Html.TextBoxFor(m => m.ngo_DateOfRegistration, new {@class = "class="input-append date"" })
  
                         @Html.ValidationMessageFor(model => model.ngo_DateOfRegistration) 
                          
                         
                           <input type="date" class="form-control" name="ngo_DateOfRegistration" id="DateOfRegistration" /
                   </div>
                        </div>*@


                    @*  Changed By Rishiraj Start*@
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="DateOfRegistration">Date Of Registration:* </label>
                        <div class="col-xs-6">
                            @Html.EditorFor(m => m.ngo_DateOfRegistration)
                            @Html.ValidationMessageFor(m => m.ngo_DateOfRegistration)
                            @*        <input type="date" class="form-control" id="JoiningDate" name="dateOfregistration" />*@
                        </div>
                    </div>
                    @*  Changed By Rishiraj END*@



                    <div class="form-group">
                        <label class="control-label col-xs-4" for="CopyOfRegistration">Copy Of Registration:*</label>
                        <div class="col-xs-6">
