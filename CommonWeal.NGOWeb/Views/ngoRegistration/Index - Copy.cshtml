                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="NGO Type">NGO Type:*</label>
                        <div class="col-xs-6">

                            @Html.DropDownListFor(m => m.ngo_Type, (SelectList)ViewBag.Country, new { @class = "form-control" })<br />
                            @Html.ValidationMessageFor(a => a.ngo_Type)

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="FCRARegisterationNumber">FCRA Registeration No:* </label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_fcraNumber)<br />
                            @* <input type="text" class="form-control" name="ngo_fcraNumber" id="FCRARegisterationNumber" placeholder="FCRA Registeration Number" />*@
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="AreaOfInterest">Area Of Interest:* </label>


                        <div class="col-xs-6">
                            @*<input type="checkbox" name="ngo_AreaOfIntrest" class="form-control" id="AreaOfInterest" value="Chidren" style="font-size: 17px" />Children
          <input type="checkbox" name="ngo_AreaOfIntrest" class="form-control" id="Ageing" value="Chidren" />Ageing
          <input type="checkbox" class="form-control" name="ngo_AreaOfIntrest" id="Food" value="Chidren" />Food
          <input type="checkbox" class="form-control" name="ngo_AreaOfIntrest" id="Women" value="Chidren" />Women
          <input type="checkbox" class="form-control" name="ngo_AreaOfIntrest" id="Health" value="Chidren" />Health
          <input type="checkbox" class="form-control" name="ngo_AreaOfIntrest" id="Education" value="Chidren" />Education*@
                            @Html.DropDownListFor(m => m.ngo_AreaOfIntrest, (SelectList)ViewBag.Country, new { @class = "form-control" })<br />
                            @Html.ValidationMessageFor(a => a.ngo_AreaOfIntrest)
                            @* @foreach (var oa in ViewBag.Areaofinterest)
                                {
                                    <option value=''>@oa.AreaOfInterest1</option>
                                }

                            </select>*@

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="OprationalArea">Operational Area (City):*</label>
                        <div class="col-xs-6">
                            @* <input list="oprationalareacity" name="ngo_OperationalAera" placeholder="Operational Area (City)" class="form-control" id="oprationalarea" />
                              @Html.ValidationMessageFor(a => a.ngo_OperationalAera)*@
                            @* <datalist id="oprationalareacity">
                                    <option value="Local">
                                    <option value="Community">
                                    <option value="National">
                                    <option value="International">
                                </datalist>*@
                            @Html.DropDownListFor(m => m.ngo_OperationalAera, (SelectList)ViewBag.Country, new { @class = "form-control" })<br />
                            @Html.ValidationMessageFor(a => a.ngo_OperationalAera)
                            @*  @foreach (var qa in ViewBag.value)
                                {
                                    <option value=''>@qa.operationalArea1</option>
                                }

                            </select>*@
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="WebsiteURL">Website URL:* </label>
                        <div class="col-xs-6">
                            @Html.TextBoxFor(a => a.ngo_WebsiteUrl)<br />
                            @*<input type="url" class="form-control" name="ngo_WebsiteUrl"placeholder="Website URL" id="WebsiteURL" />*@
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="pwd">Password:*</label>

                        <div class="col-xs-6">
                            @Html.EditorFor(a => a.ngo_password)<br />
                            @* <input type="password" class="form-control" id="pwd" name="ngo_password" placeholder="Enter password">*@
                            @Html.ValidationMessageFor(a => a.ngo_password)
                        </div>
                    </div>
                    <div class="form-group">

                        <label class="control-label col-xs-4" for="rpwd">Confirm Password:*</label>
                        <div class="col-xs-6">
                            @Html.EditorFor(a => a.ngo_password)<br />
                            @* <input type="password" class="form-control" id="rpwd" name="confirmpassword" placeholder="Confirm Password" />*@
                        </div>
                        <br />
                        <br />
                        <div class="form-group">

                            <button type="submit" class="btn btn-primary" onclick="">Submit</button><br />
                            <br />
                        </div>
                </form>
            </div>
        </div>
    </div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
        @Scripts.Render("~/bundles/jqueryui")
        @Styles.Render("~/Content/cssjqryUi")

        <script type="text/javascript">
            $(document).ready(function () {
                $('input[type=datetime]').datepicker({
                    dateFormat: "dd/M/yy",
                    changeMonth: true,
                    changeYear: true,
                    yearRange: "-60:+0"
                });

            });
        </script>

    }



</body>



