@model IEnumerable<CommonWeal.NGOWeb.ViewModel.Post>
<script src="~/Scripts/Post/PostComment.js"></script>
<div class="container">

    <div class="row">
        <div class="col-md-12 ngoprofilenavbar">
            <div class="panel panel-default  panelNGO ">
                <div class="panel-heading profileheadingcolor">
                    <center> <b class="profilesummary"> Profile Summary</b></center>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel-body" id="ngoprofilepanelbody">
                            <form method="post" action="~/NGOProfile/PostImage" enctype="multipart/form-data" id="postform">
                                <div class="row">
                                    <div class="col-md-4 col-md-push-4">
                                        @if (ViewBag.imageurl != null)
                                        {
                                            <img class="profileimage" id="profileimage" src="@ViewBag.imageurl"/>
                                            <span title="Change profile picture" id="pencil" class="glyphicon glyphicon-pencil " onclick="fileclick(); return false;"></span>
                                            <input type="file" id="file" name="file" data-badge="false" />
                                        }
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel-group navbar-fixed">
                            <ul class="ultext nav nav-tabs">
                                <li id="profilepost"><span id="NGOProfilepost">Post</span></li>
                                <li><span id="aboutus">About Us</span></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="NGOProfilecontent">
    @Html.Partial("../NGOProfile/_NGOProfilePost", Model)
</div>
















